<% if( game && game.length > 0) { %>
<% for (var i = 0; i < game.length; i ++) { %> 
        <div class="panel panel-default col-md-3">
                <div class="panel-body"><h3>Group <%= i+1 %>:00pm &nbsp;</h3><button style="background-color: #FF6B00   "><a href="/group/add/<%= game[i].id %>" id="add" style="color: white">Join</a></button>
                    <div class="row">
                        <% for (var j = 0; j < game[i].user_ids.length; j ++) { %> 
                            
                            <%= game[i].user_ids[j].name %>
                            
                        <% } %>
                    </div>
                </div>
        </div>
    <% } %>
<% } %>

